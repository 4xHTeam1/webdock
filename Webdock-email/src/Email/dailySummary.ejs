                <!--[if (mso)|(IE)]></td><![endif]-->
                <!--[if (mso)|(IE)]></tr></table></td></tr></table><![endif]-->
              </div>
            </div>
          </div> <!-- New Features -->
          <div
            style="font-family:arial,helvetica,sans-serif; max-width: 600px; margin: auto; margin-top: 20px; border: 1px solid #a9a9a9; background-color: #FFF; border-radius: 5px; padding: 20px; box-shadow: 0 0 10px rgba(0,0,0,0.15);">
            <h2 style="color: #000000; margin: 0; width: 100%; border-bottom: 1px solid #a9a9a9;">
              New Features
            </h2>
            <div
              style="clear: both; padding: 15px; border-radius: 5px;  max-height: 500px; overflow-y: auto; overflow-x: hidden;">
              <% features.forEach(function(feature) { %>
                <div
                  style="background-color: #fff; border: 1px solid #a9a9a9; border-radius: 5px; margin: 15px 0; padding: 15px">
                  <div style="margin: 10px 0px; height: 35px;">
                    <% if (feature.user.avatarURL && feature.user.avatarURL !=='' ) { %>
                      <img src="<%= feature.user.avatarURL %>" alt="<%= feature.user.name %>'s avatar"
                        style="width: 35px; height: 35px; border-radius: 50%; margin-right: 15px; float: left;">
                      <% } else { %>
                        <div
                          style="width: 35px; height: 35px; border-radius: 50%; margin-right: 15px; background-color: #ccc;  text-align: center; line-height: 35px; float: left;">
                          <span style="color: #000;">
                            <%= feature.user.name.charAt(0).toUpperCase() %>
                          </span>
                        </div>
                        <% } %>
                          <span style="display: block; color: #000000; line-height: 35px;">
                            <strong>
                              <%= feature.user.name %>
                            </strong>
                          </span>
                  </div>
                  <h3 style="color: #000000; margin: 0;">
                    <%= feature.title %>
                  </h3>
                  <p style="color: #000000; margin: 0;">
                    <%= feature.description %>
                  </p>
                  <a href="<%= feature.link %>"" target=" _blank"
                    style="display: inline-block; color: #fff; background-color: #24ab95; text-decoration: none; padding: 10px 20px; margin-top: 10px; border-radius: 5px;">
                    Go to Feature
                  </a>
                </div>

                <% }); %>
            </div>
          </div>
          <!-- End New Features -->
          <!-- New Comments-->
          <div
            style="font-family:arial,helvetica,sans-serif; max-width: 600px; margin: auto; margin-top: 20px; border: 1px solid #a9a9a9; background-color: #FFF; border-radius: 5px; padding: 20px; box-shadow: 0 0 10px rgba(0,0,0,0.15);">
            <h2 style="color: #000000; margin: 0; width: 100%; border-bottom: 1px solid #a9a9a9;">
              New Comments
            </h2>
            <div
              style="clear: both; padding: 15px; border-radius: 5px;  max-height: 500px; overflow-y: auto; overflow-x: hidden;">
              <% comments.forEach(function(comment) { %>
                <div
                  style="background-color: #fff; border: 1px solid #a9a9a9; border-radius: 5px; margin: 15px 0; padding: 15px">
                  <div style="margin: 10px 0px; height: 35px;">
                    <% if (comment.user.avatarURL && comment.user.avatarURL !=='' ) { %>
                      <img src="<%= comment.user.avatarURL %>" alt="<%= comment.user.name %>'s avatar"
                        style="width: 35px; height: 35px; border-radius: 50%; margin-right: 15px; float: left;">
                      <% } else { %>
                        <div
                          style="width: 35px; height: 35px; border-radius: 50%; margin-right: 15px; background-color: #ccc;  text-align: center; line-height: 35px; float: left;">
                          <span style="color: #000;">
                            <%= comment.user.name.charAt(0).toUpperCase() %>
                          </span>
                        </div>
                        <% } %>
                          <span style="display: block; color: #000000; line-height: 35px;">
                            <strong>
                              <%= comment.user.name %>
                            </strong>
                          </span>
                  </div>
                  <p style="color: #000000; margin: 0;">
                    <%= comment.comment %>
                  </p>
                  <% if (comment.commentReplys && comment.commentReplys.length> 0) { %>
                    <% comment.commentReplys.forEach(function(reply) { %>
                      <div style="margin-left: 20px; border-left: 1px solid #a9a9a9; padding-left: 10px;">
                        <!-- Display reply here -->
                        <div style="margin: 10px 0px; height: 35px;">
                          <% if (reply.user.avatarURL && reply.user.avatarURL !=='' ) { %>
                            <img src="<%= reply.user.avatarURL %>" alt="<%= reply.user.name %>'s avatar"
                              style="width: 35px; height: 35px; border-radius: 50%; margin-right: 15px; float: left;">
                            <% } else { %>
                              <div
                                style="width: 35px; height: 35px; border-radius: 50%; margin-right: 15px; background-color: #ccc;  text-align: center; line-height: 35px; float: left;">
                                <span style="color: #000;">
                                  <%= reply.user.name.charAt(0).toUpperCase() %>
                                </span>
                              </div>
                              <% } %>
                                <span style="display: block; color: #000000; line-height: 35px;">
                                  <strong>
                                    <%= reply.user.name %>
                                  </strong>
                                </span>
                        </div>
                        <p style="color: #000000; margin: 0;">
                          <%= reply.comment %>
                        </p>
                      </div>
                      <% }); %>
                        <% } %>
                          <a href="<%= comment.link %>"" target=" _blank"
                            style="display: inline-block; color: #fff; background-color: #24ab95; text-decoration: none; padding: 10px 20px; margin-top: 10px; border-radius: 5px;">
                            Go to comment
                          </a>
                </div>

                <% }); %>
            </div>
          </div>
          <!-- End New Comments -->

